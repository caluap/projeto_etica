@inproceedings{weldeselassie_tensor_2007,
	title = {Tensor dissimilarity based adaptive seeding algorithm for {DT}-{MRI} visualization with streamtubes},
	volume = {6509},
	url = {http://dx.doi.org/10.1117/12.710417},
	doi = {10.1117/12.710417},
	abstract = {In this paper, we propose an adaptive seeding strategy for visualization of diffusion tensor magnetic resonance imaging (DT-MRI) data using streamtubes. DT-MRI is a medical imaging modality that captures unique water diffusion properties and fiber orientation information of the imaged tissues. Visualizing DT-MRI data using streamtubes has the advantage that not only the anisotropic nature of the diffusion is visualized but also the underlying anatomy of biological structures is revealed. This makes streamtubes significant for the analysis of fibrous tissues in medical images. In order to avoid rendering multiple similar streamtubes, an adaptive seeding strategy is employed which takes into account similarity of tensors in a given region. The goal is to automate the process of generating seed points such that regions with dissimilar tensors are assigned more seed points compared to regions with similar tensors. The algorithm is based on tensor dissimilarity metrics that take into account both diffusion magnitudes and directions to optimize the seeding positions and density of streamtubes in order to reduce the visual clutter. Two recent advances in tensor calculus and tensor dissimilarity metrics are utilized: the Log-Euclidean and the J-divergence. Results show that adaptive seeding not only helps to cull unnecessary streamtubes that would obscure visualization but also do so without having to compute the culled streamtubes, which makes the visualization process faster.},
	urldate = {2017-01-04},
	author = {Weldeselassie, Yonas T. and Hamarneh, Ghassan and Weiskopf, Daniel},
	year = {2007},
	pages = {65092M--65092M--9},
	file = {Weldeselassie et al. - 2007 - Tensor dissimilarity based adaptive seeding algori.pdf:C\:\\Users\\Pichau\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ysf99hw7.default\\zotero\\storage\\7FHHK5VJ\\Weldeselassie et al. - 2007 - Tensor dissimilarity based adaptive seeding algori.pdf:application/pdf}
}

@article{zhang_asymmetric_2009,
	title = {Asymmetric {Tensor} {Analysis} for {Flow} {Visualization}},
	volume = {15},
	issn = {1077-2626},
	doi = {10.1109/TVCG.2008.68},
	abstract = {The gradient of a velocity vector field is an asymmetric tensor field which can provide critical insight that is difficult to infer from traditional trajectory-based vector field visualization techniques. We describe the structures in the eigenvalue and eigenvector fields of the gradient tensor and how these structures can be used to infer the behaviors of the velocity field. To illustrate the structures in asymmetric tensor fields, we introduce the notions of eigenvalue and eigenvector manifolds. These concepts afford a number of theoretical results that clarify the connections between symmetric and antisymmetric components in tensor fields. In addition, these manifolds naturally lead to partitions of tensor fields, which we use to design effective visualization strategies. Both eigenvalue manifold and eigenvector manifold are supported by a tensor reparameterization with physical meaning. This allows us to relate our tensor analysis to physical quantities such as rotation, angular deformation, and dilation, which provide physical interpretation of our tensor-driven vector field analysis in the context of fluid mechanics. To demonstrate the utility of our approach, we have applied our visualization techniques and interpretation to the study of the Sullivan vortex as well as computational fluid dynamics simulation data.},
	number = {1},
	journal = {IEEE Transactions on Visualization and Computer Graphics},
	author = {Zhang, E. and Yeh, H. and Lin, Z. and Laramee, R. S.},
