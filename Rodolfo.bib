	journal = {Journal of the Neurological Sciences},
	author = {Zhao, Mangsuo and Shi, Bingxin and Chen, Tuoyu and Zhang, Yuqi and Geng, Tongchao and Qiao, Liyan and Zhang, Mingjie and He, Le and Zuo, Huancong and Wang, Guihuai},
	month = apr,
	year = {2017},
	keywords = {Astrocytoma, Cervical spinal cord, diffusion tensor imaging, Diffusion tensor tractography},
	pages = {43--51},
	annote = {Imagem Interessante},
	file = {ScienceDirect Snapshot:C\:\\Users\\Pichau\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ysf99hw7.default\\zotero\\storage\\IIQAAKNI\\S0022510X1730045X.html:text/html;Zhao et al. - 2017 - Axial MR diffusion tensor imaging and tractography.pdf:C\:\\Users\\Pichau\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ysf99hw7.default\\zotero\\storage\\FC3DDTKR\\Zhao et al. - 2017 - Axial MR diffusion tensor imaging and tractography.pdf:application/pdf}
