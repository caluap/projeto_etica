@article{waller_white-matter_nodate,
	title = {White-matter tract abnormalities and antisocial behavior: {A} systematic review of diffusion tensor imaging studies across development},
	issn = {2213-1582},
	shorttitle = {White-matter tract abnormalities and antisocial behavior},
	url = {//www.sciencedirect.com/science/article/pii/S2213158217300141},
	doi = {10.1016/j.nicl.2017.01.014},
	abstract = {Antisocial behavior (AB), including aggression, violence, and theft, is thought be underpinned by abnormal functioning in networks of the brain critical to emotion processing, behavioral control, and reward-related learning. To better understand the abnormal functioning of these networks, research has begun to investigate the structural connections between brain regions implicated in AB using diffusion tensor imaging (DTI), which assesses white-matter tract microstructure. This systematic review integrates findings from 22 studies that examined the relationship between white-matter microstructure and AB across development. In contrast to a prior hypothesis that AB is associated with greater diffusivity specifically in the uncinate fasciculus, findings suggest that adult AB is associated with greater diffusivity across a range of white-matter tracts, including the uncinate fasciculus, inferior fronto-occipital fasciculus, cingulum, corticospinal tract, thalamic radiations, and corpus callosum. The pattern of findings among youth studies was inconclusive with both higher and lower diffusivity found across various association, commissural, and projection and thalamic tracts.},
	urldate = {2017-01-25},
	journal = {NeuroImage: Clinical},
	author = {Waller, Rebecca and Dotterer, Hailey L. and Murray, Laura and Maxwell, Andrea M. and Hyde, Luke W.},
	keywords = {Antisocial behavior, Callous-unemotional traits, diffusion tensor imaging, Neuroimaging, Psychopathy, Systematic review},
	file = {ScienceDirect Snapshot:C\:\\Users\\Pichau\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ysf99hw7.default\\zotero\\storage\\MC2PMBHQ\\S2213158217300141.html:text/html;Waller et al. - White-matter tract abnormalities and antisocial be.pdf:C\:\\Users\\Pichau\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ysf99hw7.default\\zotero\\storage\\ABJBV5MU\\Waller et al. - White-matter tract abnormalities and antisocial be.pdf:application/pdf}
